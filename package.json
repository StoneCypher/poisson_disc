{
  "name": "poisson_disc",
  "version": "0.1.0",
  "description": "Fast poisson disc sampling in arbitrary dimensions (poisson range, poisson sphere, etc) in node javascript / typescript",
  "main": "dist/pdisc.es5.min.js",
  "scripts": {
    "clean": "rimraf -f build/ && rimraf -f docs/ && mkdir build && cd build && mkdir gen && cd .. && mkdir docs && cd docs && mkdir grammar && cd ..",
    "test": "npm run eslint && npm run nyc",
    "nyc": "nyc ava src/tests/*.js",
    "nyc_verbose": "ava src/tests/*.js -v",
    "rollup": "rollup -c",
    "minify": "uglifyjs ./build/fslp.js -o ./build/fslp.min.js --compress",
    "eslint": "eslint src/js/*.js",
    "doc": "echo todo",
    "site": "cp -r ./src/site/* ./docs",
    "make": "npm run clean && npm run make_peg && npm run rollup && npm run minify",
    "build": "npm run make && npm run test && npm run doc && npm run site",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "travis": "npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StoneCypher/poisson_disc.git"
  },
  "keywords": [
    "poisson",
    "disc",
    "range",
    "sphere",
    "poisson_disc",
    "poisson_range",
    "poisson_sphere",
    "poisson-disc",
    "blue",
    "noise",
    "blue_noise",
    "blue-noise",
    "packing",
    "distribution",
    "placement",
    "noise",
    "texture",
    "generator",
    "typescript",
    "stonecypher"
  ],
  "author": "John Haugeland <stonecypher@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StoneCypher/poisson_disc/issues"
  },
  "homepage": "https://github.com/StoneCypher/poisson_disc#readme",
  "devDependencies": {
    "@types/node": "^9.6.1",
    "ava": "^0.25.0",
    "ava-spec": "^1.1.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-stonecypher": "^1.15.9",
    "eslint-plugin-ava": "^4.5.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-jsdoc": "^3.5.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-new-with-error": "^1.1.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-unicorn": "^4.0.2",
    "nyc": "^11.6.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "uglify-es": "^3.3.9"
  }
}
